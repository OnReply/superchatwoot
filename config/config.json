{
    "secretKey": "$SECRET_KEY",
    "host": "$HOST",
    "port": "$PORT",
    "startAllSession": true,
    "tokenStoreType": "redis",
    "debug": true,
    "deviceName": "ChatWoot",
    "whatsappVersion": null,
    "webhook": { },
    "archive": {
        "enable": false,
        "waitTime": 10,
        "daysToArchive": 45
    },
    "log": {
        "level": "$LOG_LEVEL",
        "logger": [
            "console", "file"
        ]
    },
    "createOptions": {
        "browserArgs": [
            "--no-sandbox"
        ]
    },
    "mapper": {
        "enable": true,
        "prefix": "tagone-"
    },
    "db": {
        "redisHost": "$REDIS_HOST",
        "redisPort": "$REDIS_PORT",
        "redisPassword": "$REDIS_PASSWORD",
        "redisDb": 0,
        "redisPrefix": "whatsapp:"
    }
}